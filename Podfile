# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ShoppingBuddy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for ShoppingBuddy
  pod 'GooglePlaces'
  pod 'Firebase/Core'
  pod 'Firebase/AdMob'
  pod 'Firebase/Messaging'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'

  target 'ShoppingBuddyTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GooglePlaces'
    pod 'Firebase/Core'
    pod 'Firebase/AdMob'
    pod 'Firebase/Messaging'
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'
    
  end

  target 'ShoppingBuddyUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GooglePlaces'
    pod 'Firebase/Core'
    pod 'Firebase/AdMob'
    pod 'Firebase/Messaging'
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'
    
  end

end
